{
  "name": "bigal",
  "version": "2.2.0",
  "description": "A fast and lightweight orm for postgres.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "mocha -r ts-node/register tests/**/*.tests.ts",
    "lint": "tslint --fix -p tsconfig.lint.json -c tslint.json -t stylish",
    "dist": "if [ -d \"src\" ]; then rm -rf dist && npm run build && npm run lint && npm run test && cp package.json dist && cp *.md dist && cp LICENSE dist && cp .npmignore dist && cd dist && npm publish; fi",
    "prepublishOnly": "if [ -d \"src\" ]; then echo \"Please use: npm run dist\" && exit 125; fi"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpirik/bigal.git"
  },
  "keywords": [
    "orm",
    "postgres",
    "postgresql"
  ],
  "author": "Jim Geurts <jim@biacreations.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mpirik/bigal/issues"
  },
  "homepage": "https://github.com/mpirik/bigal#readme",
  "engines": {
    "node": ">=8.10"
  },
  "dependencies": {
    "@types/lodash": "~4.14.136",
    "@types/node": "~10.14.8",
    "@types/pg": "~7.4.14",
    "lodash": "~4.17.15",
    "pg": "~7.12.0",
    "postgres-pool": "~1.4.1"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/faker": "4.1.5",
    "@types/mocha": "5.2.7",
    "chai": "4.2.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.2",
    "faker": "4.1.0",
    "husky": "2.4.0",
    "mocha": "6.2.0",
    "strict-event-emitter-types": "2.0.0",
    "ts-mockito": "2.4.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "3.5.3"
  }
}
