{
  "name": "bigal",
  "version": "2.0.3",
  "description": "A fast and lightweight orm for postgres.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "mocha -r ts-node/register tests/**/*.tests.ts",
    "lint": "tslint --fix -p tsconfig.lint.json -c tslint.json -t stylish",
    "prepublishOnly": "npm run build && npm run lint && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpirik/bigal.git"
  },
  "keywords": [
    "orm",
    "postgres",
    "postgresql"
  ],
  "author": "Jim Geurts <jim@biacreations.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mpirik/bigal/issues"
  },
  "homepage": "https://github.com/mpirik/bigal#readme",
  "engines": {
    "node": ">=8.10"
  },
  "dependencies": {
    "lodash": "~4.17.11",
    "pg": "7.10.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/faker": "4.1.5",
    "@types/lodash": "4.14.123",
    "@types/mocha": "5.2.6",
    "@types/node": "11.13.8",
    "@types/pg": "7.4.14",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "faker": "4.1.0",
    "husky": "2.2.0",
    "mocha": "6.1.4",
    "postgres-pool": "1.3.0",
    "strict-event-emitter-types": "2.0.0",
    "ts-mockito": "2.3.1",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "3.4.5"
  }
}
